{
  "name": "vscode-tfs",
  "main": "./lib/extension",
  "version": "0.7.0",
  "description": "TFS Visual Studio Code integration.",
  "author": {
    "name": "Evgeniy Generalov",
    "email": "e.generalov@gmail.com",
    "url": "https://github.com/generalov"
  },
  "publisher": "generalov",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "tfs",
    "team",
    "foundation",
    "server",
    "microsoft"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/generalov/vscode-tfs.git"
  },
  "bugs": {
    "url": "https://github.com/generalov/vscode-tfs/issues"
  },
  "homepage": "https://github.com/generalov/vscode-tfs#readme",
  "readmeFilename": "README.md",
  "engines": {
    "vscode": "^1.20.1"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1"
  },
  "devDependencies": {
    "eslint": "4.18.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-prettier": "2.6.0",
    "mocha": "^5.0.1",
    "prettier": "^1.11.0",
    "vscode": "^1.1.10"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "build": "vsce package"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-tfs.list",
        "title": "TFS: List Commands"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "TFS configuration",
      "properties": {
        "tfs.location": {
          "type": "string",
          "required": false
        }
      }
    }
  },
  "displayName": "TFS",
  "icon": "res/icon.png",
  "galleryBanner": {
    "color": "#912a2a",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ]
}
